<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta content="width=device-width, initial-scale=1.0" name="viewport">
    <title>@ViewData["Title"] - Placement_Preparation</title>
    <meta name="description" content="">
    <meta name="keywords" content="">

    <!-- Favicons -->
    <link href="~/student/assets/img/favicon.png" rel="icon">
    <link href="~/student/assets/img/apple-touch-icon.png" rel="apple-touch-icon">

    <!-- Fonts -->
    <link href="https://fonts.googleapis.com" rel="preconnect">
    <link href="https://fonts.gstatic.com" rel="preconnect" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Roboto:ital,wght@0,100;0,300;0,400;0,500;0,700;0,900;1,100;1,300;1,400;1,500;1,700;1,900&family=Inter:wght@100;200;300;400;500;600;700;800;900&family=Nunito:ital,wght@0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,200;1,300;1,400;1,500;1,600;1,700;1,800;1,900&display=swap" rel="stylesheet">

    <!-- Vendor CSS Files -->
    <link href="~/student/assets/vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet">
    <link href="~/student/assets/vendor/bootstrap-icons/bootstrap-icons.css" rel="stylesheet">
    <link href="~/student/assets/vendor/aos/aos.css" rel="stylesheet">
    <link href="~/student/assets/vendor/glightbox/css/glightbox.min.css" rel="stylesheet">
    <link href="~/student/assets/vendor/swiper/swiper-bundle.min.css" rel="stylesheet">

    <!-- Main CSS File -->
    <link href="~/student/assets/css/main.css" rel="stylesheet">
    <link href="~/student/assets/css/card.css" rel="stylesheet"/>
    <link href="~/css/table.css" rel="stylesheet"/>

    

    <!-- Font Awesome -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.7.1/css/all.min.css" integrity="sha512-5Hs3dF2AEPkpNAR7UiOHba+lRSJNeM2ECkwxUIxC1Q/FLycGTbNapWXB4tP889k5T5Ju8fs4b1P5z/iB4nMfSQ==" crossorigin="anonymous" referrerpolicy="no-referrer" />
    
    <!-- Showdown Library for Markdown Conversion -->
  <script src="https://cdnjs.cloudflare.com/ajax/libs/showdown/1.9.1/showdown.min.js"></script>

  <!-- Highlight.js Library and Styles -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/highlight.js/11.8.0/styles/default.min.css">
    <script src="https://cdnjs.cloudflare.com/ajax/libs/highlight.js/11.8.0/highlight.min.js"></script>
   

</head>

<body class="index-page toggle-sidebar">

     <!-- Include TempData Notifications Partial View -->
        <partial name="_Notification" />

    <header id="header" class="header d-flex align-items-center fixed-top">
        <div class="header-container container-fluid container-xl position-relative d-flex align-items-center justify-content-between">

            <a asp-action="Home" asp-controller="Home" asp-area="Student" class="logo d-flex align-items-center me-auto me-xl-0">
                <!-- Uncomment the line below if you also wish to use an image logo -->
                <!-- <img src="assets/img/logo.png" alt=""> -->
                <h1 class="sitename">PP</h1>
            </a>

            <nav id="navmenu" class="navmenu">
                <ul>
                    <li><a asp-action="Home" asp-controller="Home" asp-area="Student" class="active">Home</a></li>
                   <li><a href="/Student/Course/ListAllCourse?courseType=programing">Programing</a></li>
                   <li><a href="/Student/Course/ListAllCourse?courseType=Aptitude">Aptitude</a></li>
                   <li><a href="/Student/Question/ListAllCourses">Interview Questions</a></li>
                   <li><a href="/Student/Mcq/ListAllCourses">Important Mcqs</a></li>
                    @* <li class="dropdown">
                        <a href="#"><span>Aptitude</span> <i class="bi bi-chevron-down toggle-dropdown"></i></a>
                        <ul>
                            <li><a href="#">Quantitative</a></li>
                            <li><a href="#">Reasoning</a></li>
                        </ul>
                    </li> *@
                </ul>
                <i class="mobile-nav-toggle d-xl-none bi bi-list"></i>
            </nav>

            <a class="btn-getstarted" asp-area="Authentication" asp-controller="Auth" asp-action="SignIn">Log In</a>

        </div>
    </header>

    <main class="main">
        @RenderBody()
    </main>

    <footer id="footer" class="footer">

        <div class="container copyright text-center mt-4">
            <p>© <span>Copyright</span> <strong class="px-1 sitename">Placement Prepration</strong> <span>All Rights Reserved</span></p>
            <div class="credits">
                <!-- All the links in the footer should remain intact. -->
                <!-- You can delete the links only if you've purchased the pro version. -->
                <!-- Licensing information: https://bootstrapmade.com/license/ -->
                <!-- Purchase the pro version with working PHP/AJAX contact form: [buy-url] -->
            </div>
        </div>

    </footer>

    <!-- Scroll Top -->
    <a href="#" id="scroll-top" class="scroll-top d-flex align-items-center justify-content-center"><i class="bi bi-arrow-up-short"></i></a>

    <!-- Vendor JS Files -->
    <script src="~/student/assets/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
    <script src="~/student/assets/vendor/php-email-form/validate.js"></script>
    <script src="~/student/assets/vendor/aos/aos.js"></script>
    <script src="~/student/assets/vendor/glightbox/js/glightbox.min.js"></script>
    <script src="~/student/assets/vendor/swiper/swiper-bundle.min.js"></script>
    <script src="~/student/assets/vendor/purecounter/purecounter_vanilla.js"></script>

    <!-- Main JS File -->
    <script src="~/student/assets/js/main.js"></script>

 <!-- user in Markup string html convert   -->
    <script>
        document.addEventListener('DOMContentLoaded', (event) => {
            hljs.highlightAll();
            document.querySelectorAll('pre code').forEach((block) => {
                hljs.highlightElement(block);
            });
        });
    </script>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>

    <!--client side Validation-->
    <script src="~/lib/jquery-validation/dist/jquery.validate.min.js"></script>
    <script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.min.js"></script>

    <!--Notyf Notification Toast-->
    @await Component.InvokeAsync("Notyf")
    <!-- Include Notyf JS -->
    @* <script src="https://cdn.jsdelivr.net/npm/notyf/notyf.min.js"></script> *@

    @await RenderSectionAsync("Scripts", required: false)



</body>

</html>